{
  "name": "setbased/php-stratum-sqlite-pdo",
  "description": "PhpStratum SQLite: A stored procedure loader and wrapper generator for SQLite",
  "keywords": [
    "PhpStratum",
    "SQLite",
    "Stored Routines",
    "Stored Procedures",
    "Stored Functions",
    "Wrapper",
    "Loader"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "ext-pdo": "*",
    "setbased/exception": "^2.1",
    "setbased/helper-cast": "^2.0",
    "setbased/helper-code-store-php": "^2.3",
    "setbased/php-stratum-backend": "^5.2",
    "setbased/php-stratum-common": "^6.0",
    "setbased/php-stratum-middle": "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "phing/phing": "^2.0",
    "setbased/php-stratum": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Stratum\\SqlitePdo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Stratum\\SqlitePdo\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
