{
  "name": "setbased/php-stratum-sqlite-pdo",
  "description": "PhpStratum SQLite: A stored procedure loader and wrapper generator for SQLite",
  "keywords": [
    "PhpStratum",
    "SQLite",
    "Stored Routines",
    "Stored Procedures",
    "Stored Functions",
    "Wrapper",
    "Loader"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-json": "*",
    "ext-pdo": "*",
    "setbased/exception": "^2.5.0",
    "setbased/helper-cast": "^3.0.1",
    "setbased/helper-code-store-php": "^2.7.0",
    "setbased/php-stratum-backend": "^6.2.0",
    "setbased/php-stratum-common": "^7.2.0",
    "setbased/php-stratum-middle": "^5.13.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22",
    "setbased/php-stratum": "^6.4.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Stratum\\SqlitePdo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Stratum\\SqlitePdo\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
